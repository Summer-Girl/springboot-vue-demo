;AACvB,QAAA,MAAM,EAAE;AACN,UAAA,OAAO,EAAE,KAAK,WADR;AAEN,UAAA,QAAQ,EAAE,KAAK,QAFT;AAGN,UAAA,MAAM,EAAE,KAAK;AAHP;AADe,OAAzB,EAMG,IANH,CAMQ,UAAA,GAAE,EAAK;AACb,QAAA,KAAI,CAAC,SAAL,GAAiB,GAAG,CAAC,IAAJ,CAAS,OAA1B;AACA,QAAA,KAAI,CAAC,KAAL,GAAa,GAAG,CAAC,IAAJ,CAAS,KAAtB;AACD,OATD;AAUD,KAnBM;AAoBP,IAAA,GApBO,iBAoBD;AACJ,WAAK,aAAL,GAAqB,IAArB;AACA,WAAK,IAAL,GAAY,EAAZ;AACD,KAvBM;AAwBP,IAAA,IAxBO,kBAwBA;AACL,UAAI,KAAK,IAAL,CAAU,EAAV,KAAiB,IAAjB,IAAyB,KAAK,IAAL,CAAU,EAAV,KAAiB,CAA9C,EAAiD;AAC/C,QAAA,OAAO,CAAC,IAAR,CAAa,WAAb,EAA0B,KAAK,IAA/B,EAAqC,IAArC,CAA0C,UAAA,GAAE,EAAK;AAC/C,UAAA,OAAO,CAAC,GAAR,CAAY,GAAZ;AACD,SAFD;AAGF,OAJA,MAIO;AACL,QAAA,OAAO,CAAC,IAAR,CAAa,gBAAb,EAA+B,KAAK,IAApC,EAA0C,IAA1C,CAA+C,UAAA,GAAE,EAAK;AACpD,UAAA,OAAO,CAAC,GAAR,CAAY,GAAZ;AACD,SAFD;AAGF;;AACA,WAAK,aAAL,GAAqB,KAArB;AACA,WAAK,IAAL;AACD,KApCM;AAqCP,IAAA,UArCO,sBAqCI,CArCJ,EAqCO;AACZ,MAAA,OAAO,CAAC,GAAR,CAAY,CAAZ;AACA,WAAK,aAAL,GAAqB,IAArB;AACA,WAAK,IAAL,GAAY,CAAZ;AACD,KAzCM;AA0CP,IAAA,gBA1CO,4BA0CU,QA1CV,EA0CoB;AACzB,WAAK,QAAL,GAAgB,QAAhB;AACA,MAAA,OAAO,CAAC,GAAR,CAAY,KAAK,QAAjB;AACA,WAAK,IAAL;AACD,KA9CM;AA+CP,IAAA,mBA/CO,+BA+Ca,WA/Cb,EA+C0B;AAC/B,WAAK,WAAL,GAAmB,WAAnB;AACA,WAAK,IAAL;AACF;AAlDO,GAFI;AAsDb,EAAA,OAtDa,qBAsDH;AAAC;AACT,SAAK,IAAL;AACD,GAxDY;AAyDb,EAAA,IAzDa,kBAyDN;AACL,WAAO;AACL,MAAA,IAAI,EAAE,EADD;AAEL,MAAA,aAAa,EAAE,KAFV;AAGL,MAAA,WAAW,EAAE,CAHR;AAIL,MAAA,QAAQ,EAAE,EAJL;AAKL,MAAA,KAAK,EAAE,CALF;AAML,MAAA,MAAM,EAAE,EANH;AAOL,MAAA,SAAS,EAAE;AAPN,KAAP;AASF;AAnEa,CAAf","sourcesContent":["<template>\n  <!--  tableData要传的值 prop属性对应对象属性 label是表格名称 width不写自适应-->\n  <div style=\"padding: 10px\">\n    <!--    功能区域-->\n    <div style=\"margin: 10px\">\n      <el-button type=\"primary\" v-on:click=\"add\">新增</el-button>\n      <el-button type=\"primary\">导入</el-button>\n      <el-button type=\"primary\">导出</el-button>\n    </div>\n    <!--    搜索区域-->\n    <div style=\"margin: 10px\">\n      <el-input v-model=\"search\" placeholder=\"请输入关键字\" style=\"width: 20%\"></el-input>\n      <el-button type=\"primary\" @click=\"load\" style=\"margin-left: 5px\">查询</el-button>\n    </div>\n    <el-table\n        :data=\"tableData\"\n        border\n        stripe\n        style=\"width: 100%\">\n      <el-table-column\n          prop=\"id\"\n          label=\"ID\"\n          sortable\n      >\n      </el-table-column>\n      <el-table-column\n          prop=\"username\"\n          label=\"用户名\">\n      </el-table-column>\n      <el-table-column\n          prop=\"nickName\"\n          label=\"昵称\">\n      </el-table-column>\n      <el-table-column\n          prop=\"age\"\n          label=\"年龄\">\n      </el-table-column>\n      <el-table-column\n          prop=\"sex\"\n          label=\"性别\">\n      </el-table-column>\n      <el-table-column\n          prop=\"address\"\n          label=\"地址\">\n      </el-table-column>\n      <el-table-column\n          fixed=\"right\"\n          label=\"操作\"\n          width=\"180\">\n        <template #default=\"scope\">\n          <el-button @click=\"handleEdit(scope.row)\" type=\"success\">编辑</el-button>\n          <el-popconfirm\n              confirmButtonText='好的'\n              cancelButtonText='不用了'\n              icon=\"el-icon-info\"\n              iconColor=\"red\"\n              title=\"这是一段内容确定删除吗？\"\n          >\n            <template #reference>\n              <el-button type=\"danger\" @click=\"deleted(scope.$,scope.row.id)\">删除</el-button>\n            </template>\n          </el-popconfirm>\n        </template>\n      </el-table-column>\n    </el-table>\n    <div style=\"margin:10px 0;float: right\">\n      <el-pagination\n          @size-change=\"handleSizeChange\"\n          @current-change=\"handleCurrentChange\"\n          :current-page=\"currentPage\"\n          :page-size=\"pageSize\"\n          :page-sizes=\"[1,2,5,10]\"\n          layout=\"total, sizes, prev, pager, next, jumper\"\n          :total=\"total\"\n      >\n      </el-pagination>\n\n      <el-dialog\n          title=\"提示\"\n          v-model=\"dialogVisible\"\n          width=\"30%\">\n        <el-form ref=\"form\" :model=\"form\" label-width=\"30%\">\n          <el-form-item label=\"用户名\">\n            <el-input v-model=\"form.username\" style=\"width: 80%\"></el-input>\n          </el-form-item>\n          <el-form-item label=\"昵称\">\n            <el-input v-model=\"form.nickName\" style=\"width: 80%\"></el-input>\n          </el-form-item>\n          <el-form-item label=\"年龄\">\n            <el-input v-model=\"form.age\" style=\"width: 80%\"></el-input>\n          </el-form-item>\n          <el-form-item label=\"性别\">\n            <el-radio-group v-model=\"form.sex\" style=\"width: 80%\">\n              <el-radio label=\"男\">男</el-radio>\n              <el-radio label=\"女\">女</el-radio>\n              <el-radio label=\"未知\">未知</el-radio>\n            </el-radio-group>\n          </el-form-item>\n          <el-form-item label=\"住址\">\n            <el-input type=\"textarea\" v-model=\"form.address\" style=\"width:80%\"></el-input>\n          </el-form-item>\n        </el-form>\n        <template #footer>\n      <span class=\"dialog-footer\">\n         <el-button @click=\"dialogVisible = false\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"save\">确 定</el-button>\n      </span>\n        </template>\n      </el-dialog>\n\n    </div>\n  </div>\n</template>\n\n<script>\n// @ is an alias to /src\n\nimport request from \"../utils/request\";\n\nexport default {\n  name: 'Home',\n  methods: {\n    deleted(id) {\n      request.delete(\"/api/user/delete/\" + id).then(res => {\n            console.log(res)\n          }\n      )\n      this.load()\n    },\n    load() {\n      request.get(\"/api/user\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        this.tableData = res.data.records\n        this.total = res.data.total\n      });\n    },\n    add() {\n      this.dialogVisible = true\n      this.form = {}\n    },\n    save() {\n      if (this.form.id === null || this.form.id === 0) {\n        request.post(\"/api/user\", this.form).then(res => {\n          console.log(res)\n        });\n      } else {\n        request.post(\"/api/user/edit\", this.form).then(res => {\n          console.log(res)\n        });\n      }\n      this.dialogVisible = false\n      this.load()\n    },\n    handleEdit(a) {\n      console.log(a);\n      this.dialogVisible = true\n      this.form = a\n    },\n    handleSizeChange(pageSize) {\n      this.pageSize = pageSize;\n      console.log(this.pageSize)\n      this.load()\n    },\n    handleCurrentChange(currentPage) {\n      this.currentPage = currentPage\n      this.load()\n    }\n  },\n  created() {//页面加载时候调用的方法\n    this.load();\n  },\n  data() {\n    return {\n      form: {},\n      dialogVisible: false,\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      search: \"\",\n      tableData: []\n    }\n  }\n}\n</script>\n"],"sourceRoot":""}]}